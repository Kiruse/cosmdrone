{
  "name": "cosmdrone",
  "version": "0.1.0",
  "description": "Multitool for Cosmos SDK based blockchains",
  "type": "module",
  "types": "dist/lib/index.d.ts",
  "repository": "https://github.com/kiruse/cosmdrone.git",
  "author": "Kiruse",
  "license": "LGPL-3.0",
  "main": "dist/lib/index.js",
  "bin": "./dist/cli/cosmdrone.js",
  "scripts": {
    "dev": "node dist/cli/cosmdrone.js",
    "watch:lib": "tsc -p tsconfig.lib.json -w",
    "watch:cli": "coffee -c scripts/build-cli.coffee && node scripts/build-cli.js -w",
    "build": "yarn build:lib && yarn build:cli",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:cli": "coffee -c scripts/build-cli.coffee && node scripts/build-cli.js",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.30.1",
    "@cosmjs/crypto": "^0.30.1",
    "@cosmjs/encoding": "^0.30.1",
    "@cosmjs/json-rpc": "^0.30.1",
    "@cosmjs/ledger-amino": "^0.30.1",
    "@cosmjs/proto-signing": "^0.30.1",
    "@kiruse/jdf-core": "^0.2.4-alpha.1",
    "@scure/bip32": "^1.3.0",
    "@scure/bip39": "^1.2.0",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "elliptic": "^6.5.4",
    "htmlparser2": "^9.0.0",
    "keytar": "^7.9.0",
    "node-watch": "^0.7.3",
    "ora": "^6.3.1",
    "protobuf.js": "^1.1.2",
    "rimraf": "^5.0.1",
    "semver": "^7.5.1",
    "yaml": "^2.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/node": "^20.3.1",
    "@types/semver": "^7.5.0",
    "coffeescript": "^2.7.0",
    "typescript": "^5.1.3"
  }
}
